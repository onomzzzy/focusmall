 max price is set\n           let max = Math.max(pricez[0],pricez[1]);\n           let min = Math.min(pricez[0],pricez[1]);\n\n         let split:string[] = search.split(' ');\n         console.log(split);\n          //check match\n  \n          for(let i=0;i<data.length;i++){\n          //\n             //filter with price without discount\n             if((data[i].discount > 0)){\n              let newprice:number = this.getNewPrice(data[i].price,data[i].discount);\n             if((newprice >= min) && newprice <= max){\n               dataPrice.push(data[i]);\n               break;\n              }\n            }\n           else{\n              if((data[i].price >= min) && data[i].price <= max){\n              dataPrice.push(data[i]);\n             break;\n            }\n           }\n          }\n\n          //refill\n          this.searchItem = [];\n          this.searchItem = dataPrice; \n        }\n    \n    });\n  }\n\n  onClickItem(id:String){\n    for(let i=0;i<this.searchItem.length;i++){\n      if(id == this.searchItem[i].id){\n        this.itemService.setItemDisplay(this.searchItem[i]);\n        this.itemService.setPreviousPage('/home/search');\n        this.router.navigate(['/home/product']);\n      }\n    }\n   }\n\n\n  getNewPrice(price:number,discount:number){\n    let x:number = price * (discount/100);\n    return (price - x);\n  }\n\n}\n","import { Injectable } from '@angular/core';\nimport { HttpClient } from '@angular/common/http';\nimport { CookieService } from 'ngx-cookie-service';\nimport { Item } from '../classes/item';\nimport { Subject } from 'rxjs';\nimport { Order } from '../classes/order';\n\n@Injectable({\n  providedIn: 'root'\n})\nexport class ItemService {\n \n  private _searchword = new Subject<string>();\n  _searchword$ = this._searchword.asObservable();\n  //Search with price \n  private _pricesearch = new Subject<number[]>();\n  _pricesearch$ = this._pricesearch.asObservable(); \n\n  //ON/OFF price filter\n  private _isfilter = 