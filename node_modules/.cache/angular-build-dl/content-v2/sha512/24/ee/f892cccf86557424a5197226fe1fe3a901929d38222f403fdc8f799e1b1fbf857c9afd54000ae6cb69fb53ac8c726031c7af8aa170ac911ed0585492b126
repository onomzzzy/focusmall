null,\n  cable:cable,\n  flashDrive:null,\n  enclosure:null,\n  mouse:null,\n  touchpad:null,\n  headset:null,\n  gamepad:null\n}\n\nlet item:Item ={\n id:this.getId(posted.cableType,posted.cableSize),\n brand:null,\n picture:pictures,\n condition:posted.condition,\n title:title,\n description:this.autoDes?null:posted.description,\n search:search.toLowerCase(),\n laptop:null,\n peripheral:null,\n accessary:accessary,\n software:null,\n itemlocation:posted.location,\n quantity:posted.quantity,\n price:posted.price,\n newprice:newprice,\n discount:posted.discount,\n online:true\n}\nthis.aws.uploadItem(item,'FocusMall');\n\n}\n\n }\n\n getId(brand:String,model:String):String{\n  let date:Date = new Date();  \nreturn brand+'-'+date.getTime().toString()+'-'+ model;\n}\n\n\n\n}\n","\n<div class=\"page\" style=\"padding-top:.5em;\">\n\n    <div class=\"message_panel\">\n\n     <!--body-->\n     <div>\n         <p-card [style]=\"{width: '100%', border: '1px solid #b3c2ca'}\">\n             <div>\n                 <div *ngIf=\"cartItems.length == 0\">\n                     <div>\n                        <div class=\"alert alert-primary\">\n                            <strong>Empty!</strong> Your cart is Empty.\n                          </div>\n                     </div>\n                 </div>\n                <ul>\n                    <li *ngFor=\"let cart of cartItems\">\n                        <p-card [style]=\"{width: '100%', border: '1px solid #b3c2ca'}\">\n                        <div class=\"image-container\">\n                            <img  src=\"{{cart.picture[0]}}\" alt=\"\" />\n                        </div>\n                        <div class=\"seperator\"></div>\n                        <div class=\"message-box\">\n                            <span class=\"messager\">{{cart.id}} </span>\n                            <span class=\"time\"></span>   \n                            <span class=\"message\">{{cart.title}}</span>\n                            <span 