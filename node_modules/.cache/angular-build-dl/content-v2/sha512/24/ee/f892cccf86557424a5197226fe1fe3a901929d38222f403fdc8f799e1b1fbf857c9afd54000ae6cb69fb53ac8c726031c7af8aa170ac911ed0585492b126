\">\n                            <div class=\"col-12\">\n                                <textarea rows=\"5\" placeholder=\"Description in Detials\" formControlName=\"description\" pInputTextarea autoResize=\"autoResize\"></textarea>  \n                            </div>\n                        </div>\n\n                        <!--Auto Description-->\n\n\n\n                        \n                         <!--Upload Pictures-->\n\n                         <div class=\"row rowlar\">\n                            <h6>Upload Pictures</h6>   \n                        </div>\n\n                        <div class=\"row\" style=\"text-align: center\">\n                            <div class=\"col\">\n                                <input type=\"file\" style=\"display:none;\"  (change)=\"onUpload($event,0)\" #fileInput>\n                                <img class=\"upload-image\" src=\"{{pix}}\" (click)=\"fileInput.click()\">\n                            </div>\n                            <div class=\"col\">\n                                    <input type=\"file\" style=\"display:none;\"  (change)=\"onUpload($event,1)\" #fileInput1>\n                                    <img class=\"upload-image\" src=\"{{pix1}}\"\n                                    (click)=\"fileInput1.click()\">\n                            </div>\n                           <div class=\"col\">\n                                <input type=\"file\" style=\"display:none;\"  (change)=\"onUpload($event,2)\" #fileInput2>\n                                    <img class=\"upload-image\" src=\"{{pix2}}\"\n                                    (click)=\"fileInput2.click()\">\n                            </div>\n\n                            <div class=\"col\">\n                                <input type=\"file\" style=\"display:none;\"  (change)=\"onUpload($event,3)\" #fileInput3>\n                                    <img  src=\"{{pix3}}\" class=\"upload-image\" (click)=\"fileInput3.click()\">\n              