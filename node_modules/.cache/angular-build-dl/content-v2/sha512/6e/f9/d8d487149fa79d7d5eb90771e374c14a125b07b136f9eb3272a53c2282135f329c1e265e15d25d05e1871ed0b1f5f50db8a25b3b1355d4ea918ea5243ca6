ccessary:null,\n    software:null,\n    itemlocation:posted.location,\n    quantity:posted.quantity,\n    price:posted.price,\n    newprice:newprice,\n    discount:posted.discount,\n    online:true\n  }\n  this.aws.uploadItem(item,'FocusMall');\n\n }\n\n  }\n\n  getId(brand:String,model:String):String{\n    let date:Date = new Date();  \n  return brand+'-'+date.getTime().toString()+'-'+ model;\n  }\n\n}\n","<div class=\"content-section implementation carousel-demo\">\n    <p-carousel *ngIf=\"discounted && discounted.length > 0\" [value]=\"discounted\" [numVisible]=\"4\" [numScroll]=\"3\" [circular]=\"false\" [responsiveOptions]=\"responsiveOptions\">\n        <ng-template let-dis pTemplate=\"item\">\n            <div class=\"card mb-2 caro-card\" (click)=\"onClickItem(dis)\">\n                <img class=\"card-img-top\" src=\"{{dis.picture[0]}}\"\n                  alt=\"\">\n                <div class=\"card-body\" style=\"padding:5px;text-align:left;\">\n                  <h6 class=\"card-title\" style=\"font-weight: 600;\">{{dis.title | shortTitle:0}}</h6>\n                  <!--Discount-->\n                  <div *ngIf=\"dis.discount > 0\">\n                    <div class=\"p-grid\">\n                      <div class=\"p-col-8\">\n                        <h6 class=\"card-text\">{{dis.newprice | currency:'NGN':'symbol-narrow':'4.0-0'}}</h6>\n                        <h6  class=\"card-text\" style=\"text-decoration: line-through;\">{{dis.price | currency:'NGN':'symbol-narrow':'4.0-0'}}</h6>\n                      </div>\n                      <div class=\"p-col-4\">\n                        <h5 class=\"card-text\" style=\"color: red;\"><i>{{dis.discount}}%</i></h5>\n                      </div>\n                    </div>\n                   </div>\n                   <!--Discount-->\n                    <!--NonDiscount-->\n                    <div  *ngIf=\"dis.discount == 0\">\n                      <div>\n                       <h6>{{dis.price | currency:'NGN':'s